{
  "name": "Amazonas Product Cart",
  "version": "1.0.0",
  "description": "A close of Amazon\'s product add to cart",
  "author": "vinnyA3",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "client:dev": "webpack-dev-server",
    "server": "nodemon server/index.js"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{js, jsx}",
      "!**/node_modules/**",
      "!./public/**"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 50,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "jest": "^24.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "puppeteer-core": "^1.12.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
